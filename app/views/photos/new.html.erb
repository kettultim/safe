<% content_for :title, 'New Photo' %>

<%= link_to 'Back to Photos', space_photos_path(@space) %>

<%= simple_form_for @space_photo do |f| %>
  <div class="form-group">
    <%= f.input :image %>
  </div>
  <div class="form-group">
    <%= f.submit 'Upload', class: 'btn btn-default' %>
  </div>
<% end %>
